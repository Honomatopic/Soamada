{% trans_default_domain 'FOSUserBundle' %}
<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}L'association Soamada 28{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">
            <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.4.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
            <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-social/5.1.1/bootstrap-social.css" rel="stylesheet">
            <link rel="stylesheet" href="{{app.request.schemeAndHttpHost ~ asset('css/bootswatch/bootstrap.css') }}" type="text/css">
            <link rel="stylesheet" href="{{app.request.schemeAndHttpHost ~ asset('css/style.css') }}" type="text/css">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ app.request.schemeAndHttpHost ~ asset('favicon.ico') }}" />
    </head>
   
{% block body %}
    <div class="modal-fade" id="membre" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title" id="monModalMembre">
                    </h4>
                </div>
                <div class="modal-body">
                    {% block fos_user_content %}
                    {% endblock fos_user_content %}
                </div>
            </div>
        </div>
    </div>

{% endblock %}
 <footer class="container center-block police">&copy {{ "now"|date("Y") }} Andria</footer>
        {% block javascripts %}
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
        <link rel="stylesheet" href="https://code.jquery.com/ui/1.7.3/themes/base/jquery-ui.css">
        <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
        <script src="{{app.request.schemeAndHttpHost ~  asset('js/scrollmenu.js') }}"></script>
        <script src="{{app.request.schemeAndHttpHost ~  asset('js/autocompleteregistration.js') }}"></script>
        <script src="{{app.request.schemeAndHttpHost ~  asset('js/autocompleteprofile.js') }}"></script>
        <script src="{{app.request.schemeAndHttpHost ~  asset('js/autocompletedon.js') }}"></script>
        <script src="{{app.request.schemeAndHttpHost ~  asset('js/modal_connecter.js') }}"></script>
        <script src="{{app.request.schemeAndHttpHost ~  asset('js/modal_enregistrer.js') }}"></script>
    {% endblock %}
</body>
</html>


